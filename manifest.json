{
  "manifest_version": 2,

  "name": "Remove Fixed Headers",
  "description": "This extension removes fixed headers from a website.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["remove-header.css"]
    }
  ],
  "web_accessible_resources": ["remove-header.css"]
}
